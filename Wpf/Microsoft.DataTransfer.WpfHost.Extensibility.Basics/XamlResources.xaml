<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:System="clr-namespace:System;assembly=mscorlib">

  <ResourceDictionary.MergedDictionaries>
    <ResourceDictionary Source="CommonStyles.xaml"/>
  </ResourceDictionary.MergedDictionaries>

  <!-- String resources-->

  <System:String x:Key="OkKey">OK</System:String>
  <System:String x:Key="CancelKey">Cancel</System:String>

  <System:String x:Key="AddFilesKey">Add Files</System:String>
  <System:String x:Key="AddFolderKey">Add Folder</System:String>
  <System:String x:Key="AddSingleFolderKey">Single</System:String>
  <System:String x:Key="AddRecursiveFolderKey">Recursive</System:String>
  <System:String x:Key="AddUrlKey">Add URL(s)</System:String>
  <System:String x:Key="RemoveFilesKey">Remove</System:String>

  <System:String x:Key="AddUrlDialogTitleKey">Add URL(s)</System:String>

  <System:String x:Key="SelectFileKey">Select...</System:String>

  <System:String x:Key="AddItemKey">Add</System:String>
  <System:String x:Key="RemoveItemsKey">Remove</System:String>

</ResourceDictionary>